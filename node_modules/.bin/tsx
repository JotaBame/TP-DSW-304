#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules/tsx/dist/node_modules:/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules/tsx/node_modules:/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules:/mnt/d/Facultad/TP-DSW-304/node_modules:/mnt/d/Facultad/node_modules:/mnt/d/node_modules:/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules/tsx/dist/node_modules:/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules/tsx/node_modules:/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules:/mnt/d/Facultad/TP-DSW-304/node_modules:/mnt/d/Facultad/node_modules:/mnt/d/node_modules:/mnt/d/Facultad/TP-DSW-304/TP-DSW-304/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../tsx/dist/cli.mjs" "$@"
fi
